<p id="notice"><%= notice %></p>

<h1>Belongs To Collections</h1>


<table style="width:100%;">
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody style="width: 100%;">
    <% @belongs_to_collections.each do |belongs_to_collection| %>
      <tr id="belongs">
        <td id="bel"></td>
        <td><%= link_to '', belongs_to_collection, class:"far fa-eye fa-2x" %></td>
        <td><%= link_to '', edit_belongs_to_collection_path(belongs_to_collection), class: "fas fa-edit fa-2x" %></td>
        <td><%= link_to '', belongs_to_collection, method: :delete, data: { confirm: 'Are you sure?' }, class:"fas fa-trash-alt fa-2x" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Belongs To Collection', new_belongs_to_collection_path, class: "button" %>
